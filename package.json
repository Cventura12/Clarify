{
  "name": "clarify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:push": "dotenv -e .env.local -- prisma db push",
    "prisma:gen": "dotenv -e .env.local -- prisma generate",
    "db:sync": "npm run prisma:push && npm run prisma:gen",
    "eval:engine": "dotenv -e .env.local -- node --loader ts-node/esm src/lib/evals/run.ts"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.2",
    "next": "14.2.35",
    "next-auth": "^4.24.13",
    "openai": "^4.0.0",
    "react": "^18",
    "react-dom": "^18",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "prisma": "^6.19.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
